        "payload": {
          "id": "51664a21-1299-491f-8cbc-02a30035f37d",
          "checkType": "MULTI_STEP",
          "name": "Example Multistep Check",
          "frequency": 60,
          "frequencyOffset": 497,
          "activated": true,
          "muted": false,
